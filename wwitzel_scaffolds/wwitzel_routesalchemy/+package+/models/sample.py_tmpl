import sqlalchemy as sa
from {{project}}.models import Base

class Sample(Base):
    __tablename__ = 'sample'
    id = sa.Column(sa.Integer, primary_key=True)
    name = sa.Column(sa.Unicode(255))
    value = sa.Column(sa.Integer)
    
    def __init__(self, name, value):
        self.name = name
        self.value = value
